{
  "name": "@hamq/testlab",
  "description": "A collection of test utilities for AMQP",
  "version": "0.1.5",
  "keywords": [],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=14"
  },
  "author": "TY",
  "license": "MIT",
  "scripts": {
    "build": "nb-tsc",
    "clean": "nb-clean hamq-testlab*.tgz dist *.tsbuildinfo package",
    "pretest": "npm run clean && npm run build",
    "test": "npm run jest",
    "jest": "jest",
    "verify": "npm pack && tar xf hamq-testlab*.tgz && tree package && npm run clean"
  },
  "devDependencies": {
    "@nutol/build": "^0.2.4",
    "@nutol/eslint-config": "^0.2.1",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.18",
    "docker-cli-js": "^2.9.0"
  },
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "publishConfig": {
    "access": "public"
  }
}
